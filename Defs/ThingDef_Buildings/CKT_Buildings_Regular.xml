  <ThingDef ParentName="ArtBuildingBase" Name="CKT_SculptureBase" Abstract="True">
    <minifiedDef>MinifiedThing</minifiedDef>
    <thingCategories>
      <li>BuildingsSpecial</li>
    </thingCategories>
    <thingSetMakerTags><li>RewardStandardLowFreq</li></thingSetMakerTags>
    <statBases>
      <SellPriceFactor>1.10</SellPriceFactor>
    </statBases>
    <comps>
      <li Class="CompProperties_Art">
        <nameMaker>NamerArtSculpture</nameMaker>
        <descriptionMaker>ArtDescription_Sculpture</descriptionMaker>
        <canBeEnjoyedAsArt>true</canBeEnjoyedAsArt>
      </li>
    </comps>
    <placeWorkers>
      <li>PlaceWorker_MeditationOffsetBuildingsNear</li>
    </placeWorkers>
    <tradeTags>
      <li>Art</li>
    </tradeTags>
    <ideoBuildingNamerBase MayRequire="Ludeon.RimWorld.Ideology">NamerIdeoSculpture</ideoBuildingNamerBase>
    <building>
      <paintable>true</paintable>
    </building>
  </ThingDef>
  
  <ThingDef ParentName="CKT_SculptureBase">
    <defName>CKT_1984_MonteCarlo</defName>
    <label>1984 monte carlo statue</label>
    <description>A huge sculpture of an ancient absolutely banging vehicle. May summon lot lizzards. Looks pretty cool. "yea thats my whip, aint she sweet".</description>
    <castEdgeShadows>true</castEdgeShadows>
    <graphicData>
      <graphicClass>Graphic_Multi</graphicClass>
      <texPath>Crackhead_King_Trader/1984_Monte_Carlo</texPath>
      <drawSize>(4,4)</drawSize>
    </graphicData>
    <fillPercent>0.5</fillPercent>
    <rotatable>false</rotatable>
    <size>(2,2)</size>
    <canOverlapZones>false</canOverlapZones>
    <statBases>
      <MaxHitPoints>300</MaxHitPoints>
      <Beauty>1000</Beauty>
      <Mass>28</Mass>
      <WorkToMake>105000</WorkToMake>
      <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">40</StyleDominance>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  
  <ThingDef ParentName="CKT_SculptureBase">
    <defName>CKT_Terry</defName>
    <label>terry fargo mclean the 7th</label>
    <description>Terry Fargo McLean The 7th. "last of his name and greatest in fame baby". No one knows why its called that but it does look really interesting!</description>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/Art/SculptureSmall</texPath>
    </graphicData>
    <fillPercent>0.35</fillPercent>
    <rotatable>false</rotatable>
    <statBases>
      <MaxHitPoints>90</MaxHitPoints>
      <Mass>3</Mass>
      <Beauty>200</Beauty>
      <WorkToMake>18000</WorkToMake>
      <StyleDominance MayRequire="Ludeon.RimWorld.Ideology">10</StyleDominance>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>
  
</Defs>
