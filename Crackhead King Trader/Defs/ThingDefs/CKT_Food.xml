<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ResourceBase">
    <defName>CKT_RedBowl</defName>
    <label>Redbowl</label>
    <description>A delicious super caffinated drink often paired with liqor. You get the strange feeling that copyright lawyers are staring at you when you say the name out loud. It fulfills the need for recreation, but it is not very nutritious.</description>
    <graphicData>
      <texPath>Things/Item/Drug/Beer</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
      <drawSize>0.88</drawSize>
	  <color>(34, 57, 231)</color>
    </graphicData>
    <soundInteract>Standard_Drop</soundInteract>
    <soundDrop>Standard_Drop</soundDrop>
    <socialPropernessMatters>true</socialPropernessMatters>
    <statBases>
      <MaxHitPoints>60</MaxHitPoints>
      <MarketValue>3</MarketValue>
      <Mass>0.075</Mass>
      <Flammability>1.0</Flammability>
      <DeteriorationRate>8</DeteriorationRate>
      <Nutrition>0.1</Nutrition>
    </statBases>
    <thingCategories>
      <li>Foods</li>
    </thingCategories>
    <ingestible>
      <preferability>DesperateOnly</preferability>
      <foodType>Processed</foodType>
      <joy>0.23</joy>
      <joyKind>Gluttonous</joyKind>
      <maxNumToIngestAtOnce>4</maxNumToIngestAtOnce>
      <defaultNumToIngestAtOnce>4</defaultNumToIngestAtOnce>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
    <allowedArchonexusCount>80</allowedArchonexusCount>
    <possessionCount>10</possessionCount>
  </ThingDef>
  
  <ThingDef ParentName="MealCooked">
    <defName>CKT_Burrito</defName>
    <label>burrito</label>
    <description>A partially eaten burrito with what appears to be a bell on the packaging. "You'll be fine. It ages like fine wine". The same nutrition as a simple meal. Smells delicious</description>
    <graphicData>
      <texPath>Crackhead_King_Trader/Burrito</texPath>
      <graphicClass>Graphic_StackCount</graphicClass>
	  <drawSize>0.88</drawSize>
	  <color>(241, 222, 209)</color>
    </graphicData>
    <statBases>
      <MarketValue>15</MarketValue>
      <WorkToMake>300</WorkToMake>
      <Nutrition>0.9</Nutrition>
    </statBases>
    <ingestible>
      <preferability>MealSimple</preferability>
      <ingestEffect>EatVegetarian</ingestEffect>
      <ingestSound>Meal_Eat</ingestSound>
    </ingestible>
  </ThingDef>
  
</Defs>

