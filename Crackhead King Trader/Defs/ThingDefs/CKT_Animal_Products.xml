<?xml version="1.0" encoding="utf-8"?>
<Defs>
    <ThingDef ParentName="EggUnfertBase">
    <defName>CKT_EggTrogloditeUnfertilized</defName>
    <label>troglodite egg (unfert.)</label>
    <description>An unfertilized troglodite egg. It can be eaten raw, but it's much, much better cooked.</description>
    <graphicData>
      <color>(189, 225, 240)</color>
    </graphicData>
    <statBases>
      <MarketValue>5.0</MarketValue>
    </statBases>
  </ThingDef>

  <ThingDef ParentName="EggFertBase">
    <defName>CKT_EggTrogloditeFertilized</defName>
    <label>troglodite egg (fert.)</label>
    <description>A fertilized troglodite egg. If all goes well, it should hatch into a chick. It can be eaten raw, but it's much better cooked.</description>
    <possessionCount>1</possessionCount>
    <graphicData>
      <color>(244, 214, 211)</color>
    </graphicData>
    <statBases>
      <MarketValue>5.0</MarketValue>
    </statBases>
    <comps>
      <li Class="CompProperties_Hatcher">
        <hatcherDaystoHatch>2</hatcherDaystoHatch>
        <hatcherPawn>CKT_Troglodite</hatcherPawn>
      </li>
    </comps>
  </ThingDef>
  
</Defs>

